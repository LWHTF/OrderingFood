<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.liezh.ssh.domain.Menus" table="menus">
		<id name="mid" column="mid">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name" length="20"></property>
		<property name="price" column="price" length="20"></property>
		<property name="state" column="state" length="4"></property>
		<property name="synopsis" column="synopsis" length="300"></property>
		<property name="material" column="material" length="200"></property>
	</class>


</hibernate-mapping>
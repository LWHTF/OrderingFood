<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.liezh.ssh.domain.Users" table="users">
		<id name="uid" column="uid">
			<generator class="native"></generator>
		</id>
		<property name="username" column="username" length="20"></property>
		<property name="password" column="password" length="20"></property>
		<property name="phoneNum" column="phoneNum" length="20"></property>
	</class>


</hibernate-mapping>